{
  "version": "2",
  "sql": [{
    "schema": "sql/migrations",
    "queries": "sql/query.sql",
    "engine": "postgresql",
    "gen": {
      "go": {
        "out": "/internal/db",
        "emit_empty_slices" : true,
        "emit_json_tags": true,
        "emit_prepared_queries":true,
        "sql_package": "pgx/v5"
      }
    }
  }]
}
